{
  "name": "peppermint-router",
  "description": "Lightweight hash router for React",
  "version": "0.1.0",
  "author": "Alon Bar",
  "license": "MIT",
  "repository": "https://github.com/alonrbar/peppermint-router.git",
  "main": "dist/peppermint-router.cjs.js",
  "module": "dist/peppermint-router.esm.js",
  "files": [
    "dist",
    "src/**/*.{ts,tsx}"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"./{src,test}/**/!(*.d).ts\"",
    "test": "del dist-test && rollup -c rollup.config.test.js -w",
    "release": "yarn typecheck && yarn lint && del dist && mkdir dist && rollup -c"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.5.0",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.3",
    "@babel/plugin-proposal-optional-catch-binding": "7.2.0",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/node": "10.1.4",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "babel-plugin-annotate-pure-calls": "0.4.0",
    "babel-plugin-ts-nameof": "0.3.0",
    "del-cli": "1.1.0",
    "eslint": "5.16.0",
    "rollup": "1.17.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-serve": "1.0.1",
    "typescript": "3.5.3"
  }
}
